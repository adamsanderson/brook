{
  "name": "brook-feed-reader",
  "version": "0.2.9",
  "description": "The Brook feed reeder",
  "main": "index.js",
  "scripts": {
    "build": "webpack -w --display-error-details --progress --colors --mode=development",
    "build-prod": "webpack -w --display-error-details --progress --colors --mode=production",
    "analyze": "webpack --mode=production --profile --json > webpack-profile-data.json && webpack-bundle-analyzer webpack-profile-data.json extension/dist",
    "start": "web-ext run -s extension/ --url about:debugging",
    "start-verbose": "web-ext run --verbose -s extension/",
    "lint": "eslint src",
    "lint-package": "web-ext lint -s extension/",
    "package": "webpack --mode=production && web-ext build -s extension/ && git archive --format=zip -o web-ext-artifacts/brook-src.zip master",
    "postpackage": "cat ./docs/publishing.md",
    "test": "jest"
  },
  "author": "Adam Sanderson",
  "license": "MPL-2.0",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.0",
    "babel-loader": "8.0.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.5.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^24.3.0",
    "web-ext": "3.0.0",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@sentry/browser": "^4.5.1",
    "feedme": "^1.0.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-fast-compare": "^2.0.1",
    "react-feather": "^1.1.1",
    "react-redux": "^6.0.1",
    "react-transition-group": "^2.2.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "webext-redux": "^2.0.1"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://example.com"
  }
}
