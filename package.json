{
  "name": "brook-feed-reader",
  "version": "0.5.1",
  "description": "The Brook feed reeder",
  "main": "index.js",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider webpack -w --progress --color",
    "clean": "rm -rf ./node_modules; rm -rf ./extensions/dist",
    "start": "yarn firefox",
    "firefox": "web-ext run -s extension/ --url about:debugging#/runtime/this-firefox",
    "lint": "eslint src test",
    "test": "jest",
    "analyze": "NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --profile --json > webpack-profile-data.json && webpack-bundle-analyzer webpack-profile-data.json extension/dist",
    "package": "NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack && web-ext build -s extension/ && git archive --format=zip -o web-ext-artifacts/brook-src.zip master",
    "postpackage": "cat ./docs/publishing.md"
  },
  "author": "Adam Sanderson",
  "license": "MPL-2.0",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^27.2.1",
    "babel-loader": "8.2.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.0.0",
    "jest": "^27.2.1",
    "web-ext": "6.4.0",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@sentry/browser": "^4.5.1",
    "buffer": "^6.0.3",
    "feedme": "^2.0.2",
    "lodash": "^4.17.5",
    "process": "^0.11.10",
    "prop-types": "^15.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-fast-compare": "^3.0.1",
    "react-feather": "^2.0.3",
    "react-redux": "^7.1.1",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "stream-browserify": "^3.0.0",
    "webext-redux": "^2.0.1"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/web-ext-artifacts/"
    ],
    "testURL": "http://example.com",
    "testEnvironment": "jsdom"
  }
}
