{
  "name": "brook-feed-reader",
  "version": "0.5.1",
  "description": "The Brook feed reader",
  "main": "index.js",
  "scripts": {
    "build": "vite build --watch",
    "build:prod": "NODE_ENV=production vite build",
    "clean": "rm -rf ./node_modules; rm -rf ./extension/dist",
    "start": "yarn firefox",
    "firefox": "web-ext run -s ./dist/ --url about:debugging#/runtime/this-firefox",
    "lint": "eslint src test",
    "test": "jest",
    "analyze": "NODE_ENV=production vite build && npx vite-bundle-analyzer extension/dist",
    "package": "yarn build:prod && web-ext build -s extension/ && git archive --format=zip -o web-ext-artifacts/brook-src.zip master",
    "postpackage": "cat ./docs/publishing.md"
  },
  "author": "Adam Sanderson",
  "license": "MPL-2.0",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@vitejs/plugin-react": "^5.0.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^30.1.2",
    "babel-loader": "10.0.0",
    "buffer": "^6.0.3",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "file-loader": "^6.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "stream-browserify": "^3.0.0",
    "vite": "^7.1.5",
    "vite-plugin-web-extension": "^4.4.5",
    "web-ext": "8.9.0",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@sentry/browser": "^4.5.1",
    "feedme": "^2.0.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-fast-compare": "^3.0.1",
    "react-feather": "^2.0.3",
    "react-redux": "^7.1.1",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "webext-redux": "^2.0.1"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/web-ext-artifacts/"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://example.com"
    }
  },
  "packageManager": "yarn@4.9.4"
}
