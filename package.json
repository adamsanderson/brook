{
  "name": "brook-feed-reader",
  "version": "0.1.6",
  "description": "The Brook feed reeder",
  "main": "index.js",
  "scripts": {
    "build": "webpack -w --display-error-details --progress --colors --mode=development",
    "build-prod": "webpack -w --display-error-details --progress --colors --mode=production",
    "analyze": "webpack --mode=production --profile --json > webpack-profile-data.json && webpack-bundle-analyzer webpack-profile-data.json extension/dist",
    "start": "web-ext run -s extension/",
    "start-verbose": "web-ext run --verbose -s extension/",
    "lint": "eslint src",
    "lint-package": "web-ext lint -s extension/",
    "package": "webpack --display-error-details --progress --colors --mode=production && web-ext build -s extension/",
    "test": "jest"
  },
  "author": "Adam Sanderson",
  "license": "MPL-2.0",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "8.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.5.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^23.5.0",
    "web-ext": "2.9.1",
    "webpack": "4.17.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "feedme": "^1.0.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "react": "16.5.0",
    "react-chrome-redux": "^2.0.0-alpha.2",
    "react-dom": "16.5.0",
    "react-fast-compare": "^2.0.1",
    "react-feather": "^1.1.1",
    "react-redux": "^5.0.6",
    "react-transition-group": "^2.2.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://example.com"
  }
}
