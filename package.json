{
  "name": "brook-feed-reader",
  "version": "0.5.1",
  "description": "The Brook feed reader",
  "main": "index.js",
  "scripts": {
    "build": "NODE_OPTIONS='--openssl-legacy-provider' webpack -w --display-error-details --progress --colors",
    "clean": "rm -rf ./node_modules; rm -rf ./extensions/dist",
    "start": "yarn firefox",
    "firefox": "web-ext run -s ./extension/ --firefox=/Applications/Firefox.app/Contents/MacOS/firefox --url about:debugging#/runtime/this-firefox",
    "lint": "eslint src test",
    "test": "jest",
    "analyze": "NODE_ENV=production webpack --profile --json > webpack-profile-data.json && webpack-bundle-analyzer webpack-profile-data.json extension/dist",
    "package": "NODE_ENV=production webpack && web-ext build -s extension/ && git archive --format=zip -o web-ext-artifacts/brook-src.zip master",
    "postpackage": "cat ./docs/publishing.md"
  },
  "author": "Adam Sanderson",
  "license": "MPL-2.0",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^29.0.0",
    "babel-loader": "8.1.0",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "web-ext": "7.4.0",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@sentry/browser": "^4.5.1",
    "feedme": "^2.0.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-fast-compare": "^3.0.1",
    "react-feather": "^2.0.3",
    "react-redux": "^7.1.1",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "webext-redux": "^2.0.1"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/web-ext-artifacts/"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://example.com"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
